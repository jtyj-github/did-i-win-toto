generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model totoTicket {
  id      String   @id @default(auto()) @map("_id") @db.ObjectId
  date    DateTime
  numbers Int
  type    System
}

enum System {
  SYSTEM6
  SYSTEM7
  SYSTEM8
  SYSTEM9
  SYSTEM10
  SYSTEM11
  SYSTEM12
  SYSTEMROLL
}
